<odoo>
    <data>
        
        <!-- La vue Tree Paiement -->
    <record model="ir.ui.view" id="commerciale_paiement_tree">
        <field name="name">commerciale_paiement_tree</field>
        <field name="model">commerciale.paiement</field>
        <field name="arch" type="xml">
            <tree>
                <field name="memo1"/>
                <field name="montant1" />
                <field name="type" />
                <field name="date1" />
                <field name="nom_agent" />
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_commerciale_Paiement">
            <field name="name">Paiement</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">commerciale.paiement</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>